<h1><%= t("Themes") %></h1>
<br />
<table>
  <tr>
    <th><%= t("Name") %></th>
    <th><%= t("Actions") %></th>
  </tr>
<% @themes.each do |theme| %>
  <tr>
    <td><%= theme.name %></td>
    <td><%= link_to t("Edit"), edit_object_url(theme) %> | <%= link_to t("Delete"), object_url(theme), :method => :delete %></td>
  </tr>
<% end %>
</table>
<br />
<%= link_to "Create New Theme", new_object_url %>
