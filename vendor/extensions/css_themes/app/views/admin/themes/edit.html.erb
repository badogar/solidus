<h1>Edit Theme</h1>
<br />
<%= error_messages_for :theme %>
<% form_for(:theme, :url => object_url, :html => { :method => :put }) do |f| %>
<%= render :partial => "form", :locals => { :f => f } %>
<%= submit_tag "Update Theme Name" %>
<% end %>
<%= link_to "Activate Theme", :action => :generate, :id => params[:id] %>
<br /><br />
<div id="css_listing">
	<%= render :partial => 'css_points', :locals => { :css_points => @css_points, :theme_id => params[:id]} -%>
</div>
